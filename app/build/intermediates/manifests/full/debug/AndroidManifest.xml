        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/pemesanan"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="wendistian.utsmobileprogramming.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="wendistian.utsmobileprogramming.MainActivity" />
        <activity
            android:name="wendistian.utsmobileprogramming.Pemesanan"
            android:label="Data Pemesanan" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name="wendistian.utsmobileprogramming.FinishOrder"
            android:label="Finish Order" />
